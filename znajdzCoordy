var x_min = 488;
var x_max = 500;
var y_min = 470;
var y_max = 480;
var result = ""; 
var coord;

var coords = "496|473 494|475 491|456 497|473 492|478 492|453 509|455 494|478 490|458 496|480 510|476 516|495 516|492 524|496 525|492 521|495 519|488 527|491 520|496 521|490 524|492 520|498 521|496 523|491 521|491 522|490 520|500 527|494 521|497 524|491 526|493 519|500 509|454 504|463 495|478 493|477 493|474 494|474 495|473 513|457 512|458 514|458 512|459 512|455 514|456 515|456 512|460 513|454 514|460 510|460 509|460 509|456 498|458 511|455 510|457 509|452 521|456 512|453 517|468 515|467 511|466 515|468 514|465 516|468 518|469 504|464 509|461 510|464 509|464 511|462 511|453 511|452 491|458 496|456 494|458 511|475 495|458 499|460 495|454 492|473 495|479 492|476 492|474 495|474 493|452 490|470 489|469 512|473".split(" ")

for(var i = 0; i < coords.length; i++)
    {
    var coord = coords[i].split("|");
    if(x_min <= parseInt(coord[0]) && x_max >= parseInt(coord[0]) && y_min <= parseInt(coord[1]) && y_max >= parseInt(coord[1]))
        result += coords[i] + " ";
}
result = result.trim()
