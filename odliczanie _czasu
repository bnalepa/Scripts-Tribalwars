var actualTime = new Date(Date.now());
var endTime = new Date(2022,10,10)
var remaningTime = endTime.getTime() - actualTime.getTime();
var daysLeft= remaningTime/1000/60/60/24;
var secondsLeft = remaningTime/1000;

var i=parseInt(secondsLeft);
function f()
{
    i--;
    console.log(i);
}
setInterval(f,1000);
